                            <key>pavpn.gwid[{#KEY}]</key>
                            <delay>0</delay>
                            <history>7d</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <allowed_hosts>10.0.0.0/8,192.168.0.0/16,127.0.0.0/8</allowed_hosts>
                            <applications>
                                <application>
                                    <name>Palo Alto VPN</name>
